{
  "name": "bee-bestie-initialization",
  "version": "1.0.0",
  "description": "Database initialization worker for bee-bestie puzzle data",
  "type": "module",
  "main": "src/index.ts",
  "files": [
    "src",
    "migration.sql"
  ],
  "scripts": {
    "dev": "wrangler dev",
